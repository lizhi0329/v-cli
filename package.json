    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "vue": "^3.3.4"
  }
